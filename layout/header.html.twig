<header>
  <!-- Fixed navbar -->
  <nav class="navbar navbar-expand-md navbar-dark fixed-top bg-dark">
    <div class="container-fluid">
      <a class="navbar-brand" href="{{ path('app_index') }}">SymfonyBlog</a>
      <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarCollapse">
        <ul class="navbar-nav me-auto mb-2 mb-md-0">
        {% if Cheatsheet_var is not defined %}
          <li class="nav-item">
            <a class="nav-link active" aria-current="page" href="{{ path('index_cheatsheet') }}">Cheatsheet</a>
          </li>
          {% else %}
          <li class="nav-item">
            <a class="nav-link" href="#">Link</a>
          </li>
          <li class="nav-item">
            <a class="nav-link disabled">Disabled</a>
          </li>
          {% endif %}
          {% if categories is defined %}
          {% for categoryName in categories %}
          <li class="nav-item">
          <a class="nav-link active" href="{{ path('index_category', {'categoryName': categoryName.category }) }}">{{ categoryName.category }}</a>
          </li>
          {% endfor %}
          {% endif %}
        </ul>
      </div>
    </div>
  </nav>
</header>
